                        </div>
                        <hr>
                    </div>
                    <div class="table_base">
                        <form action="" method="">
                            <table>
                                <thead>
                                    <tr>
                                        <th> S.No. </th>
                                        <th> company name </th>
                                        <th> 10th percentage </th>
                                        <th> 12th percentage</th>
                                        <th> graduation percentage </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(var i=0;i<student.length;i++) { %> 
                                        <tr>
                                        <td><%= i+1 %></td>
